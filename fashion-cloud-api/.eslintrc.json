{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard",
        "plugin:jest/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": [
        "temp.js",
        "allure-report/*",
        "jest.config.js"
    ],
    "globals": {
        "reporter": true
    }
}